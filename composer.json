{
  "name": "nicedump/nicedump-twig",
  "description": "Twig extension for dumping a variable in NiceDump format",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "nice",
    "dump",
    "twig",
    "extension"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "twig/twig": "^2.4||^3.0",
    "nicedump/nicedump": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "NiceDumpTwig\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NiceDumpTwig\\Tests\\": "tests"
    }
  }
}
